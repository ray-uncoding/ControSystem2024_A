    P1 = [1 7 2 9 10 12 15];    % s^6 + 7s^5 + 2s^4 + 9s^3 + 10s^2 + 12s + 15
    P2 = [1 9 8 9 12 15 20];    % s^6 + 9s^5 + 8s^4 + 9s^3 + 12s^2 + 15s + 20
    roots_P1 = roots(P1);
    roots_P2 = roots(P2);
    
    P3 = P1 + P2;
    P4 = P1 - P2; 
    P5 = conv(P1, P2);

    % 顯示結果% 顯示結果
